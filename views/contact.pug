extend base.pug 

block scripts 
    script(src='/static/index.js')

block style 
    style 
        include ../static/style.css 
        include ../static/stylecontact.css   

block content
    div.container
      h1 CONTACT US
      form(action="/contact" method="post" class="myform")
            input(type="text", name="name", class="myinput", placeholder="Enter Your Username ")
            input(type="text" ,name="Age", class="myinput" ,placeholder="Enter Your Age ")
            input(type="text" ,name="gender", class="myinput" ,placeholder="Enter Your Gender")
            input(type="email" ,name="email" ,class="myinput" ,placeholder="Enter Your E-mail ")
            input(type="phone" ,name="phone" ,class="myinput" ,placeholder="Enter Your Phone ")
            input(type="text" ,name="address" ,class="myinput" ,placeholder="Enter Your Address ")
            button(class="btn") 
                | SUBMIT